      <X>0</X>
      <Y>1</Y>
      <Z>0.5</Z>
    </Direction>
    <Strength>10</Strength>
    <MinRadius>200</MinRadius>
    <MaxRadius>600</MaxRadius>

    <RadiusCycleScale>4</RadiusCycleScale>
    <OscillateRadius>true</OscillateRadius>
    <FollowBoneName>sleeve_bottom</FollowBoneName>
    <FollowOwnRotation>true</FollowOwnRotation>
