    <Color>
      <X>.7</X>
      <Y>0.5</Y>
      <Z>0.1</Z>
      <W>1</W>
    </Color>
    <SpecularColor>
      <X>.7</X>
      <Y>0.5</Y>
      <Z>0.1</Z>
      <W>1</W>
    </SpecularColor>
